{
    "manifest_version": 3,
    "name": "Генератор паролей",
    "version": "1.0",
    "description": "Генерация паролей с настройкой сложности и копированием в буфер обмена.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_width": 400
    },
    "permissions": ["clipboardWrite", "storage"]
  }